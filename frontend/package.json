{
  "name": "typescript-react-jsx-init",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server",
    "start:open": "webpack-dev-server --open",
    "test": "mocha tests/**/*.test.ts --opts ./mocha.opts",
    "test:coverage": "nyc --reporter=text mocha tests/**/*.test.ts --opts ./mocha.opts",
    "test:coverage:html": "nyc --reporter=html mocha tests/**/*.test.ts --opts ./mocha.opts && open .coverage/index.html"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/i18next": "^8.4.3",
    "@types/mocha": "^5.0.0",
    "@types/node": "^10.1.4",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/react-i18next": "^7.6.1",
    "@types/webpack-bundle-analyzer": "^2.9.2",
    "mocha": "^5.1.0",
    "nyc": "^11.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0",
    "source-map-support": "^0.5.4",
    "ts-loader": "^3.0.0",
    "ts-node": "^5.0.0",
    "tsconfig-paths": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.3.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "@types/date-fns": "^2.6.0",
    "@types/i18next-browser-languagedetector": "^2.0.1",
    "@types/i18next-xhr-backend": "^1.4.1",
    "@types/prop-types": "^15.5.2",
    "@types/react-redux": "^5.0.15",
    "@types/react-router": "^4.0.22",
    "@types/react-router-dom": "^4.2.4",
    "axios": "^0.19.0",
    "date-fns": "^1.29.0",
    "i18next": "^11.3.2",
    "i18next-browser-languagedetector": "^2.2.0",
    "i18next-xhr-backend": "^1.5.1",
    "material-ui": "^1.0.0-beta.34",
    "material-ui-icons": "^1.0.0-beta.17",
    "prop-types": "^15.6.1",
    "react-i18next": "^7.7.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "typescript-array-utils": "^0.1.4",
    "typescript-object-utils": "^0.3.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "html"
    ],
    "report-dir": "./.coverage",
    "all": true
  }
}
